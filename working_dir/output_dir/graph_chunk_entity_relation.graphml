<node id="Hộ Kinh Doanh và Hộ Gia Đình">
  <data key="d0">Hộ Kinh Doanh và Hộ Gia Đình</data>
  <data key="d1">cánhân</data>
  <data key="d2">Hộ kinh doanh và hộ gia đình có thể bị xử phạt như cá nhân vi phạm hành chính.</data>
  <data key="d3">chunk-c4630a2b3d12ef1652abe5c7403100e6</data>
