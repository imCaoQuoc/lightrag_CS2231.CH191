<node id="Thanh tra chuyên ngành">
  <data key="d0">Thanh tra chuyên ngành</data>
  <data key="d1">cơquannhànước</data>
  <data key="d2">Thanh tra chuyên ngành có quyền xử phạt các vi phạm hành chính trong lĩnh vực quản lý nhà nước.</data>
  <data key="d3">chunk-1866fd21675142868035483158ece5af</data>
  <data key="d4">168-nd-cp.signed.md</data>
  <data key="d5">1763987058</data>
  <data key="d6"></data>
</node>
<node id="Chánh Thanh tra Sở Giao thông vận tải">
  <data key="d0">Chánh Thanh tra Sở Giao thông vận tải</data>
  <data key="d1">cơquannhànước</data>
  <data key="d2">Chánh Thanh tra Sở Giao thông vận tải có quyền áp dụng các biện pháp xử phạt và khắc phục hậu quả trong lĩnh vực giao thông.</data>
  <data key="d3">chunk-1866fd21675142868035483158ece5af</data>
