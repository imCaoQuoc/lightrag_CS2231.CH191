  <data key="d0">Chánh Thanh tra tỉnh</data>
  <data key="d1">cơquannhànước</data>
  <data key="d2">Chánh Thanh tra tỉnh có quyền xử phạt hành chính trong trường hợp không có Thanh tra Sở Giao thông vận tải.</data>
  <data key="d3">chunk-1866fd21675142868035483158ece5af</data>
