  <data key="d1">hànghóa</data>
  <data key="d2">Hàng Hóa Nguy Hiểm bao gồm các loại hàng có thể gây hại cho sức khỏe và an toàn của hành khách.</data>
  <data key="d3">chunk-a014dfc1f4653d6871efbc74940e8bb2</data>
